import java.util.Scanner;

class Main {
  static int first;
  static int second;
  public static void main(String[] args) {
    Scanner in = new Scanner(System.in);
    System.out.println("Insert a value:");
    first = in.nextInt();
    boolean addNext = true;
    System.out.println("Insert next value:");
    
    for(int i = 1;addNext; i++){
    if (i < first){
      second = in.nextInt();
      addNext = true;
      } else {
      addNext = false;
    }
    }
  System.out.println("Min value is: " + second);
    }
  }
